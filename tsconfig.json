{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ESNext",
    "jsx": "preserve",
    "lib": ["DOM", "ES2020"],
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "incremental": true,
    "tsBuildInfoFile": "node_modules/.cache/.tsbuildinfo",
    "skipLibCheck": true
  }
}
